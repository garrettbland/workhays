{
    "name": "workhays",
    "version": "2.0.0",
    "description": "Work Hays using Firebase",
    "main": "index.js",
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "prettier": {
        "semi": false,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 70,
        "tabWidth": 4,
        "useTabs": false,
        "bracketSpacing": true
    },
    "scripts": {
        "clean": "rimraf dist",
        "serve:eleventy": "npx @11ty/eleventy --serve",
        "serve:parcel": "cross-env NODE_ENV=development parcel watch src/js/main.js --out-dir dist/assets",
        "serve:firebase": "firebase serve",
        "dev": "npm-run-all clean --parallel serve:*",
        "build:eleventy": "npx @11ty/eleventy",
        "build:parcel": "cross-env NODE_ENV=production parcel build ./src/js/main.js --out-dir dist/assets",
        "build": "npm-run-all clean build:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/garrettbland/workhays.git"
    },
    "author": "Garrett Bland",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/garrettbland/workhays/issues"
    },
    "homepage": "https://github.com/garrettbland/workhays#readme",
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.11.0",
        "alpinejs": "^2.6.0",
        "autoprefixer": "^9.8.6",
        "cross-env": "^7.0.2",
        "html-minifier": "^4.0.0",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "tailwindcss": "^1.7.2"
    },
    "dependencies": {
        "feather-icons": "^4.28.0",
        "liquidjs": "^9.15.0"
    }
}
