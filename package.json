{
  "name": "workhays",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "buildcss": "tailwind build public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "purgecss": "purgecss --config ./purgecss.config.js --output public/stylesheets",
    "uglifycss": "uglifycss public/stylesheets/style.css > public/stylesheets/style.min.css",
    "dev": "npm run buildcss && DEBUG=express-locallibrary-tutorial:* nodemon ./bin/www",
    "prod": "export NODE_ENV=production && npm run buildcss && npm run purgecss && npm run uglifycss && DEBUG=express-locallibrary-tutorial:* nodemon ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.0.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "purgecss": "^2.0.5",
    "tailwindcss": "^1.1.4",
    "uglifycss": "0.0.29"
  }
}
