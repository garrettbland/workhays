{
    "name": "workhays",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "dev": "run-p dev:**",
        "dev:arc": "cd arc && npx arc sandbox",
        "dev:next": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "arc:deploy-staging": "cd arc && npx arc deploy --tags project=workhays-staging --verbose",
        "arc:deploy-dry": "cd arc && npx arc deploy --tags project=workhays-staging --verbose --dry-run",
        "arc:deploy-prod": "cd arc && npx arc deploy --production --tags project=workhays-production --verbose"
    },
    "dependencies": {
        "amazon-cognito-identity-js": "^6.2.0",
        "next": "^13.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@architect/architect": "^10.11.0",
        "@architect/utils": "^3.1.6",
        "@faker-js/faker": "^7.6.0",
        "@testing-library/react": "^14.0.0",
        "@types/node": "18.15.11",
        "@types/react": "18.0.34",
        "@vitejs/plugin-react": "^3.1.0",
        "@vitest/coverage-istanbul": "^0.30.1",
        "autoprefixer": "^10.4.14",
        "eslint": "8.38.0",
        "eslint-config-next": "13.3.0",
        "jsdom": "^21.1.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "tailwindcss": "^3.3.1",
        "vitest": "^0.30.1"
    },
    "prettier": {
        "semi": false,
        "bracketSpacing": true,
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "trailingComma": "es5"
    },
    "eslintConfig": {
        "extends": "next/core-web-vitals"
    }
}
