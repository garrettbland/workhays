{
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "hosting": {
        "public": "dist",
        "rewrites": [
            {
                "source": "/jobs{,/**}",
                "function": "bigben"
            }
        ],
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    },
    "storage": {
        "rules": "storage.rules"
    }
}
